{
  "name": "alex-no/language-detector",
  "description": "Framework-agnostic language detector with Yii2 and Laravel adapters",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "psr/simple-cache": "^1.0 || ^2.0 || ^3.0"
  },
  "autoload": {
    "psr-4": {
      "LanguageDetector\\": "src/",
      "LanguageDetector\\Adapters\\": "src/Adapters/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5 || ^10.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-main": "0.1-dev"
    }
  }
}